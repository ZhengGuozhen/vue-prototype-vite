{
  "name": "vite-vue3-starter",
  "version": "0.0.1",
  "private": false,
  "description": "A Vite2.x + Vue3 + TypeScript template starter",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build-tsc": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "format": "prettier --write ./src",
    "lint": "eslint ./src --ext .vue,.js,.ts",
    "lint-fix": "eslint --fix ./src --ext .vue,.js,.ts",
    "prepare": "husky install",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/XPoet/vite-vue3-starter.git"
  },
  "keywords": [
    "Vite",
    "Vue3",
    "TypeScript",
    "Starter"
  ],
  "author": "XPoet",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/XPoet/vite-vue3-starter/issues"
  },
  "homepage": "https://github.com/XPoet/vite-vue3-starter#readme",
  "lint-staged": {
    "*.{vue,js,ts}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cesium": "^1.83.0",
    "d3": "^7.0.0",
    "echarts": "^5.1.2",
    "element-plus": "^1.0.2-beta.36",
    "js-base64": "^3.6.1",
    "jsplumb": "^2.15.6",
    "lodash": "^4.17.21",
    "mockjs": "^1.1.0",
    "ol": "^6.5.0",
    "three": "^0.130.0",
    "uuid": "^8.3.2",
    "vue": "^3.0.5",
    "vue-router": "^4.0.6",
    "vuex": "^4.0.0",
    "vxe-table": "^4.0.23",
    "xe-utils": "^3.3.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.9",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@types/jest": "^26.0.22",
    "@types/node": "^16.0.0",
    "@types/webpack-env": "^1.16.2",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "@vitejs/plugin-vue": "^1.2.1",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/test-utils": "^2.0.0-rc.4",
    "autoprefixer": "^10.3.1",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-alloy": "^4.2.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.15.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "node-sass": "^6.0.1",
    "postcss": "^8.3.6",
    "prettier": "^2.2.1",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "stylus": "^0.54.8",
    "tailwindcss": "^2.2.7",
    "ts-jest": "^26.5.4",
    "typescript": "^4.1.3",
    "vite": "^2.1.5",
    "vite-plugin-cesium": "^1.2.5",
    "vite-plugin-style-import": "^0.9.2",
    "vue-eslint-parser": "^7.10.0",
    "vue-jest": "^5.0.0-alpha.7",
    "vue-tsc": "^0.0.15"
  }
}
